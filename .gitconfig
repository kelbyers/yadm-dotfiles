# vim:ft=gitconfig
[alias]
	append = town append
	hack = town hack
	kill = town kill
	new-pull-request = town new-pull-request
	prepend = town prepend
	prune-branches = town prune-branches
	rename-branch = town rename-branch
	repo = town repo
	ship = town ship
	sync = town sync
[core]
	excludesfile = ~/.gitignore_global
	quotepath = false
	commitGraph = true
[commit]
	template = ~/.stCommitMsg
[git-town]
	new-branch-push-flag = true
[include]
	path = .gitconfig.user
	path = .gitconfig.local
[bulkworkspaces]
	dojo = /Users/jxb2016/git/aether-foundation/dojo/src
[pull]
	ff = only
[gc]
	writeCommitGraph = true
